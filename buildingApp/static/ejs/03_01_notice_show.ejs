<table class="table table-bordered table-condensed table-hover">
	<colgroup>
		<col width="4%">
		<col width="5%">
		<col width="5%">
		<col width="6%">
		<col width="6%">
		<col width="4%">
		<col width="7%">

		<col width="7%">
		<col width="7%">
		<col width="7%">
		<col width="7%">

		<col width="7%">
		<col width="7%">
		<col width="7%">

		<col width="6%">
		<col width="6%">
		<col width="4%">
		
		<col width="6%">
		<col width="5%">
		<col width="7%">
		<col width="7%">
		<col width="4%">
	</colgroup>
	<tr>
		<th rowspan="2" style="text-align:center; vertical-align:middle">최종 확인</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">최종 확인일</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">호수</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">입주자</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">년도/월</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">고지 회차</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">합계 금액</th>

		<th colspan="4" style="text-align:center; vertical-align:middle">임대료 및 관리비</th>
		<th colspan="3" style="text-align:center; vertical-align:middle">공과금</th>
		<th colspan="3" style="text-align:center; vertical-align:middle">연체료</th>

		<th rowspan="2" style="text-align:center; vertical-align:middle">기타 금액</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">변동 금액</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">고지서 확인</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">상세고지 내역확인</th>
		<th rowspan="2" style="text-align:center; vertical-align:middle">출력 선택</th>
	</tr>
	<tr>
		<th style="text-align:center; vertical-align:middle">임대료</th>
		<th style="text-align:center; vertical-align:middle">관리비</th>
		<th style="text-align:center; vertical-align:middle">부가세</th>
		<th style="text-align:center; vertical-align:middle">주차비</th>

		<th style="text-align:center; vertical-align:middle">전기</th>
		<th style="text-align:center; vertical-align:middle">수도</th>
		<th style="text-align:center; vertical-align:middle">가스</th>

		<th style="text-align:center; vertical-align:middle">미납금</th>
		<th style="text-align:center; vertical-align:middle">연체료</th>
		<th style="text-align:center; vertical-align:middle">회차</th>
	</tr>
	<% for (i=0; i < data.length; i++) { %>
	<tr>
		<td style="text-align:center; vertical-align:middle"><input type="checkbox" checked="true" disabled="disabled"></td>
		<td style="text-align:center; vertical-align:middle; color:green"><%= data[i].noticedate %></td>
		<td style="text-align:center; vertical-align:middle"><%= data[i].roomnum %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].name %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].yearmonth %></td>
		<td style="text-align:center; vertical-align:middle">-</td>
		<td style="text-align:center; vertical-align:middle"><%= data[i].total.toLocaleString() %></td>
		
		<td style="text-align:center; vertical-align:middle"><%=data[i].lease.toLocaleString() %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].maintenance.toLocaleString() %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].surtax.toLocaleString() %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].parking.toLocaleString() %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].electricity.toLocaleString() %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].water.toLocaleString() %></td>
		<td style="text-align:center; vertical-align:middle"><%=data[i].gas.toLocaleString() %></td>

		<td style="text-align:center; vertical-align:middle">-</td>
		<td style="text-align:center; vertical-align:middle">-</td>
		<td style="text-align:center; vertical-align:middle">-</td>
		<td style="text-align:center; vertical-align:middle">-</td>
		<td style="text-align:center; vertical-align:middle">-</td>
		<td style="text-align:center"><button class="btn btn-small btn-setting" disabled="disabled">확인</button></td>
		<td style="text-align:center"><button class="btn btn-small" onClick="showDetail(<%= data[i].buildingnum %>,<%= data[i].roomnum %>)">확인</button></td>
		<td style="text-align:center; vertical-align:middle"><input type="checkbox"></td>	
	</tr>
	<% } %>
</table>
