<% var w = new Array(5,5,6,6,6,6,6,6,6,6,6,6,6,6,6,6,6); %>
<table class="table table-bordered table-condensed" style="margin-bottom:0px">
	<tr>
		<th style="text-align:center">층</th>
		<th>호수</th>
		<th>입주자</th>
		<th>입주일</th>
		<th>입주검침</th>
		<th>사용기간</th>
		<% if (bid != 8) { %>
		<th>전월검침</th>
		<th>당월검침</th>
		<th>전월사용량</th>
		<th>당월사용량</th>
		<% } else { %>
		<th>급탕전월검침</th>
		<th>난방전월검침</th>
		<th>급탕당월검침</th>
		<th>난방당월검침</th>
		<th>급탕전월사용량</th>
		<th>난방전월사용량</th>
		<th>급탕당월사용량</th>
		<th>난방당월사용량</th>
		<% } %>
		<th>기본요금</th>
		<th>사용요금</th>
		<th>부가가치세</th>
		<th>절사금액</th>
		<th>합계금액</th>
	</tr>
	<% for (i = 0; i < data.length; i++) { %>
	<tr>
		<td style="text-align:center"><%= data[i].floor %></td>
		<td style="text-align:center"><%= data[i].roomnum %></td>
		<td style="text-align:center"><%= data[i].name %></td>
		<td style="text-align:center"><%= data[i].date %></td>
		<td style="text-align:center"><%= data[i].readIn.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].usePeriod.toLocaleString().replace('.00','') %></td>
		<% if (bid != 8) { %>
		<td style="text-align:center"><%= data[i].readBefore.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].readNow.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].capacityBefore.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].capacityNow.toLocaleString().replace('.00','') %></td>
		<% } else { %>
		<td style="text-align:center"><%= data[i].readBeforeHotWater.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].readBeforeHeat.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].readNowHotWater.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].readNowHeat.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].capacityBeforeHotWater.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].capacityBeforeHeat.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].capacityNowHotWater.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].capacityNowHeat.toLocaleString().replace('.00','') %></td>
		<% } %>
		<td style="text-align:center"><%= data[i].basicCharge.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].useCharge.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].vat.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].trimmedFee.toLocaleString().replace('.00','') %></td>
		<td style="text-align:center"><%= data[i].totalFee.toLocaleString().replace('.00','') %></td>
	</tr>
	<% } %>
</table>
